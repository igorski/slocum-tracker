<div id="songExportWindow">
    <div class="header">
        <button class="close-button">x</button>
    </div>
    <h4>Song export</h4>
    <fieldset>
        <div class="wrapper input">
            <label for="startPattern">Export song range:</label>
            <input type="number" id="exportStartPattern" min="1" max="1" value="1"> to
            <input type="number" id="exportLastPattern" min="1" max="1" value="1">
        </div>
    </fieldset>
    <button class="confirm-button">Export</button>
</div>
