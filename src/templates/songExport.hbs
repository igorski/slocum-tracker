<div id="songExportWindow">
    <div class="header">
        <button class="close-button">x</button>
    </div>
    <h4>Song export</h4>
    <fieldset>
        <div class="wrapper input">
            <label for="startPattern">Export song range:</label>
            <input type="number" id="exportStartPattern" min="1" max="1" value="1"> to
            <input type="number" id="exportLastPattern" min="1" max="1" value="1">
        </div>
    </fieldset>
    <fieldset>
        {{#if supportsBinary}}
            <button class="preview-binary">Preview song</button>
            <button class="export-binary">Export Music Kit binary</button>
        {{/if}}
        <button class="export-header">Export header file</button>
    </fieldset>
</div>
